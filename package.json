  },
  "devDependencies": {
    "babelify": "~6.4.0",
    "proxyquire": "~1.7.3",
    "tape": "~4.2.2",
    "uglifyify": "~3.0.1"
  },
  "keywords": [
    "warning",
    "warn",
    "console"
  ],
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=2.0.0"
  },
  "repository": "vweevers/node-forewarn",
  "bugs": "https://github.com/vweevers/node-forewarn/issues",
  "homepage": "https://github.com/vweevers/node-forewarn"
