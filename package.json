{
  "name": "forewarn",
  "version": "0.0.1",
  "description": "Scoped and styled warning logger",
  "license": "MIT",
  "main": "index.js",
  "browser": "browser.js",
  "files": [
    "index.js",
    "browser.js"
  ],
  "author": {
    "name": "Vincent Weevers",
    "email": "mail@vincentweevers.nl",
    "url": "vincentweevers.nl"
  },
  "scripts": {
    "test": "babel-node test.js | tap-spec",
